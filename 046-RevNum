import java.util.Scanner;
public class Revnum {
    public static void main(String[] args)
    {
        int rev=0,rem,org;
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        org=num;
        while(num!=0)
        {
            rem=num%10;
            rev=rev*10+rem;
            num=num/10;
        }
        System.out.println("The reverse of "+org+" is "+rev);
    }
}
